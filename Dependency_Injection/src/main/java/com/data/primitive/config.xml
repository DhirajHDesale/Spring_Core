<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean name="st1" class="com.data.primitive.Student">
		<property name="id">
			<value>25</value>
		</property>
		<property name="name">
			<value>Dhiraj</value>
		</property>
		<property name="city">
			<value>Pune</value>
		</property>
	</bean>
	<bean name="st2" class="com.data.primitive.Student">
		<property name="id" value="26" />
		<property name="name" value="Dinesh" />
		<property name="city" value="Hyderabaad" />
	</bean>


</beans>